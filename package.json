{
  "name": "bssm-auth",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev-json": "json5 Backend/Data/JsonServer/db.json5 > Backend/Data/JsonServer/db.json",
    "server": "json-server-auth --watch data/db.json --port 5000",
    "client": "vite",
    "dev0": "concurrently \"npm run dev-json\" \"npm run server\" \"npm run client\"",
    "dev": "concurrently \"npm run client\"",
    "predeploy": "npm run build",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "homepage": "https://bomba004.github.io/react-auth",
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.2",
    "@popperjs/core": "^2.11.8",
    "@radix-ui/react-slot": "^1.2.2",
    "@reduxjs/toolkit": "^1.9.7",
    "@sweetalert2/theme-dark": "^5.0.27",
    "@tippyjs/react": "^4.2.6",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^10.16.5",
    "i18next": "^24.2.3",
    "json-server": "^0.17.4",
    "json-server-auth": "^2.1.0",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.509.0",
    "react": "^18.2.0",
    "react-content-loader": "^7.0.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^15.4.1",
    "react-popper": "^2.3.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.19.0",
    "react-toastify": "^11.0.5",
    "recharts": "^2.9.3",
    "redux-persist": "^6.0.0",
    "sweetalert2": "^11.17.2",
    "swiper": "^11.2.6",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
